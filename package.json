{
  "name": "nursery",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "quilt lint",
    "test": "quilt test",
    "type-check": "quilt type-check",
    "develop": "quilt develop",
    "build": "quilt build",
    "deploy": "pnpm publish -r",
    "deploy:preview": "changeset version --snapshot preview && changeset publish --tag preview --no-git-tag",
    "version-bump": "changeset version && pnpm install --no-frozen-lockfile"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@quilted/browserslist-config": "^0.1.9",
    "@quilted/craft": "^0.1.229",
    "@quilted/eslint-config": "^0.2.2",
    "@quilted/prettier": "^0.2.11",
    "@quilted/testing": "^0.1.6",
    "@quilted/typescript": "^0.2.20",
    "typescript": "^5.2.2"
  },
  "prettier": "@quilted/prettier",
  "eslintConfig": {
    "extends": "@quilted/eslint-config/workspace"
  },
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
  ],
  "packageManager": "pnpm@8.1.1"
}
