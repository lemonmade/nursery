    "@quilted/typescript": "^0.3.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "rollup": "^4.16.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "prettier": "@quilted/prettier",
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
