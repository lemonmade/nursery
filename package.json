    "@quilted/prettier": "^0.2.11",
    "@quilted/rollup": "^0.2.32",
    "@quilted/typescript": "^0.4.1",
    "@quilted/vite": "^0.1.22",
    "@types/node": "^20.14.9",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "prettier": "@quilted/prettier",
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
