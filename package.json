    "@quilted/prettier": "^0.2.11",
    "@quilted/rollup": "^0.3.2",
    "@quilted/typescript": "^0.4.2",
    "@quilted/vite": "^0.2.1",
    "@types/node": "^22.15.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "vite": "^6.3.0",
    "vitest": "^3.1.4"
  },
  "prettier": "@quilted/prettier",
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
